{
  "manifest_version" : 2,

  "name": "Nophie",
  "version" : "0.0.0",
  "description" : "Require some simple math work at an interval to keep browsing.",
  
  "icons": {
    "128" : "icon.png"
  },

  "background" : {
    "scripts": ["background.js"]
  },

  "options_page" : "options.html",

  "browser_action" : {
    "default_icon" : "icon.png",
    "default_title" : "Nophie",
    "default_popup" : "index.html"
  },

  "permissions" : [
    "storage"
  ]
}
